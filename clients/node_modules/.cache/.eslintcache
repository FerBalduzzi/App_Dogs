\r\n      return 0;\r\n    }\r\n    if (orden === DES) {\r\n      if (nombreA < nombreB) {\r\n        return 1;\r\n      }\r\n      if (nombreA > nombreB) {\r\n        return -1;\r\n      }\r\n      return 0;\r\n    }\r\n  });\r\n  return function (dispatch) {\r\n    dispatch({ type: SORT_RAZA, payload: razaSort });\r\n  };\r\n}\r\n\r\nexport function filtroTemp(razaActual, temperamento) {\r\n  let filtro = [...razaActual];\r\n  filtro = filtro.filter((actual) => {\r\n    if (actual.temperament) {\r\n      let razaTemp = actual.temperament.split(\", \");\r\n      return razaTemp.includes(temperamento);\r\n    } else {\r\n      return false;\r\n    }\r\n  });\r\n  return function (dispatch) {\r\n    dispatch({ type: SORT_RAZA, payload: filtro });\r\n  };\r\n}\r\n\r\n//https://api.thedogapi.com/v1/images/search?breed_ids=1\r\n// forma de buscar por id\r\n",{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","severity":1,"message":"60","line":18,"column":9,"nodeType":"61","endLine":18,"endColumn":52},{"ruleId":"62","severity":1,"message":"63","line":42,"column":6,"nodeType":"64","endLine":42,"endColumn":8,"suggestions":"65"},{"ruleId":"59","severity":1,"message":"60","line":95,"column":13,"nodeType":"61","endLine":100,"endColumn":15},{"ruleId":"59","severity":1,"message":"60","line":166,"column":15,"nodeType":"61","endLine":166,"endColumn":64},{"ruleId":"59","severity":1,"message":"60","line":183,"column":19,"nodeType":"61","endLine":183,"endColumn":66},{"ruleId":"66","severity":1,"message":"67","line":1,"column":28,"nodeType":"68","messageId":"69","endLine":1,"endColumn":36},{"ruleId":"62","severity":1,"message":"70","line":13,"column":6,"nodeType":"64","endLine":13,"endColumn":8,"suggestions":"71"},{"ruleId":"59","severity":1,"message":"60","line":23,"column":13,"nodeType":"61","endLine":23,"endColumn":92},{"ruleId":"62","severity":1,"message":"72","line":37,"column":6,"nodeType":"64","endLine":37,"endColumn":8,"suggestions":"73"},{"rul